+
+int RIGHT_CPU(void)
+{
+     return 1;
+}
+
+#else
+
+#include <signal.h>
+#include <setjmp.h>
+
+static jmp_buf jb;
+
+static void sighandler(int x)
+{
+     longjmp(jb, 1);
+}
+
+static int really_have_neon(void)
+{
+     void (*oldsig)(int);
+     oldsig = signal(SIGILL, sighandler);
+     if (setjmp(jb)) {
+	  signal(SIGILL, oldsig);
+	  return 0;
+     } else {
+	  __asm__ __volatile__ ("vmov.f32 d0, #0.0\n" : : : "d0");
+	  signal(SIGILL, oldsig);
+	  return 1;
+     }
+     return 0;
+}
+
+int RIGHT_CPU(void)
+{
+     static int init = 0, res;
+     if (!init) {
+	  res = really_have_neon();
+	  init = 1;
+     }
+     return res;
+}
